@using RemoteFinder.Models
@{
    ViewData["Title"] = "User Social Page";
}

<div class="container">
    <div class="row">
        <a href="@Url.Action("Index", "Home")">Go to Files</a>
        <a href="@Url.Action("Create", "UserSocial")">Add New</a>
        <table class="table">
            <thead>
            <tr>
                <th scope="col">Username</th>
                <th scope="col">Email</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
            </tr>
            </thead>
            <tbody>
            @foreach (UserSocial item in Model)
            {
                <tr>
                    <th>@item.Username</th>
                    <td>@item.Email</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>